{
	"info": {
		"_postman_id": "1163550b-3f5d-4ec7-899a-8ab7127c214a",
		"name": "Hearing",
		"schema": "https://schema.getpostman.com/json/collection/v2.1.0/collection.json"
	},
	"item": [
		{
			"name": "Get Hearing",
			"event": [
				{
					"listen": "test",
					"script": {
						"exec": [
							"pm.test(\"Status code is 200\", function () {",
							"    pm.response.to.have.status(200);",
							"});",
							"",
							"pm.test(\"Hearing included\", function () {",
							"    var json_data = pm.response.json();",
							"    pm.expect(json_data).to.haveOwnProperty(\"hearing\")",
							"    pm.expect(json_data).to.haveOwnProperty(\"shared_time\")",
							"});"
						],
						"type": "text/javascript"
					}
				}
			],
			"request": {
				"method": "GET",
				"header": [
					{
						"key": "Authorization",
						"value": "Basic {{Username}}",
						"type": "text"
					}
				],
				"url": {
					"raw": "{{BaseUrl}}/v2/hearing/{{HearingId}}",
					"host": [
						"{{BaseUrl}}"
					],
					"path": [
						"v2",
						"hearing",
						"{{HearingId}}"
					],
					"query": [
						{
							"key": "",
							"value": null,
							"disabled": true
						}
					]
				}
			},
			"response": []
		}
	]
}